<div class="container-fluid">
  <div class="row">
    <div class="col-lg-12">

<div id="app">
  <h1 v-on:mouseover="MOana" class='message'> {{ message }} </h1>
  <h1 class='message'> {{ clicks }} </h1>
  <h1 class='message'> {{ inputText }} </h1>
  <h1 class='message'> {{ result }} </h1>

  <form>
    <input v-on:keyup="updateMessage" v-model="inputText" type="text" class="form-control" placeholder="Anything" />
  </form>

  <!-- Begin Earth Drawing -->
  <div class="box">
    <div class="earth">
      <div class="earth-copy">
        <div v-on:click="colorMe(2)" v-bind:style="styleSpot1" class="spot-1">
        </div>
        <div v-bind:id="controlId" class="spot-2">
        </div>
        <div class="spot-3">
        </div>
        <div class="spot-4">
        </div>
        <div class="spot-5">
        </div>
      </div>
    </div>
  </div>
</div>

    </div>
  </div>
 </div>
